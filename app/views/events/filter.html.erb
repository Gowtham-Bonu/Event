<table class="table table-striped" >
    <tr>
        <th>Event-name</th>
        <th>Event-Description</th>
        <th>Event-Date</th>
    <tr>
    <% @events.each do |event| %>
    <tr>
        <td ><%= event.name %></td>
        <td ><%= event.description %></td>
        <td ><%= event.event_date %></td>
    </tr>
    <% end %>
</table>
